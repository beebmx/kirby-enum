{
	"name": "beebmx/kirby-enum",
    "description": "Kirby Enum adds the ability to display and set enumeration content in the panel.",
    "license": "MIT",
    "type": "kirby-plugin",
    "version": "1.0.1",
    "keywords": [
        "enum",
        "enums",
        "panel-field",
        "kirby",
        "kirby-plugin",
        "kirby4",
        "kirby5"
    ],
    "authors": [
        {
            "name": "Fernando Gutierrez",
            "homepage": "https://beeb.mx"
        }
    ],
	"require": {
		"getkirby/composer-installer": "^1.1"
	},
	"require-dev": {
        "getkirby/cms": "^4.5|^5.0",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^3.0",
        "spatie/ray": "^1.0"
	},
    "autoload": {
        "psr-4": {
            "Beebmx\\KirbyEnum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "getkirby/composer-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "kirby-cms-path": false,
        "kirby-plugin-path": "tests/Fixtures/site/plugins"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
